# Development Environment Variables
VITE_BACKEND_URL=http://localhost:5002
VITE_APP_ENVIRONMENT=development
VITE_APP_NAME=Business Loan Portal (Dev)
VITE_APP_DESCRIPTION=Complete Business Loan Management System - Development
VITE_APP_VERSION=1.0.0-dev

# API Configuration
VITE_API_TIMEOUT=30000
VITE_API_RETRY_ATTEMPTS=3

# Feature Flags
VITE_ENABLE_NOTIFICATIONS=true
VITE_ENABLE_REAL_TIME_UPDATES=true
VITE_ENABLE_FILE_UPLOAD=true
VITE_ENABLE_PDF_VIEWER=true

# Security (relaxed for development)
VITE_ENABLE_CSRF_PROTECTION=false
VITE_ENABLE_RATE_LIMITING=false

# UI Configuration
VITE_DEFAULT_LANGUAGE=en
VITE_ENABLE_DARK_MODE=false
VITE_ITEMS_PER_PAGE=10

# Cache Configuration
VITE_CACHE_DURATION=300000
VITE_ENABLE_OFFLINE_MODE=false

# Debug
VITE_DEBUG_MODE=true
VITE_LOG_LEVEL=debug
